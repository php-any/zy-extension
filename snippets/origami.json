{
  "Class Definition": {
    "prefix": "class",
    "body": [
      "class ${1:Name} {",
      "    public function __construct(${2}) {",
      "        ${3}",
      "    }",
      "}"
    ],
    "description": "Origami class definition"
  },
  "Function Definition": {
    "prefix": "function",
    "body": ["function ${1:name}(${2}): ${3:void} {", "    ${0}", "}"],
    "description": "Function definition"
  },
  "Public Method": {
    "prefix": "public",
    "body": ["public function ${1:name}(${2}): ${3:void} {", "    ${0}", "}"],
    "description": "Public method definition"
  },
  "Chinese Function": {
    "prefix": "函数",
    "body": ["函数 ${1:名称}(${2}): ${3:void} {", "    ${0}", "}"],
    "description": "中文函数定义"
  },
  "Spawn Coroutine": {
    "prefix": "spawn",
    "body": ["spawn {", "    ${0}", "};"],
    "description": "Spawn a new coroutine"
  },
  "Echo Output": {
    "prefix": "echo",
    "body": ["echo \"${1}\";"],
    "description": "Output string"
  },
  "Chinese Output": {
    "prefix": "输出",
    "body": ["输出 \"${1}\";"],
    "description": "输出字符串"
  },
  "Match Expression": {
    "prefix": "match",
    "body": [
      "match (${1:condition}) {",
      "    case ${2:pattern} => ${3:result},",
      "    default => ${4:default_result}",
      "};"
    ],
    "description": "Match expression"
  },
  "If Statement": {
    "prefix": "if",
    "body": ["if (${1:condition}) {", "    ${0}", "}"],
    "description": "If statement"
  },
  "Foreach Loop": {
    "prefix": "foreach",
    "body": [
      "foreach ($${1:variable} as $${2:key} => $${3:value}) {",
      "    ${0}",
      "}"
    ],
    "description": "Foreach loop"
  },
  "Controller Annotation": {
    "prefix": "@Controller",
    "body": ["@Controller", "@Route(prefix: \"${1:/api}\")"],
    "description": "Controller and Route annotations"
  },
  "Service Annotation": {
    "prefix": "@Service",
    "body": ["@Service(\"${1:name}\")"],
    "description": "Service annotation"
  },
  "Inject Annotation": {
    "prefix": "@Inject",
    "body": ["@Inject(service: \"${1:ServiceName}\")"],
    "description": "Inject service annotation"
  },
  "Get Mapping": {
    "prefix": "@GetMapping",
    "body": ["@GetMapping(path: \"${1:/path}\")"],
    "description": "Get mapping annotation"
  },
  "Post Mapping": {
    "prefix": "@PostMapping",
    "body": ["@PostMapping(path: \"${1:/path}\")"],
    "description": "Post mapping annotation"
  },
  "Put Mapping": {
    "prefix": "@PutMapping",
    "body": ["@PutMapping(path: \"${1:/path}\")"],
    "description": "Put mapping annotation"
  },
  "Delete Mapping": {
    "prefix": "@DeleteMapping",
    "body": ["@DeleteMapping(path: \"${1:/path}\")"],
    "description": "Delete mapping annotation"
  },
  "Entity Annotation": {
    "prefix": "@Entity",
    "body": ["@Entity", "@Table(name: \"${1:table_name}\")"],
    "description": "Entity and Table annotations"
  },
  "Column Annotation": {
    "prefix": "@Column",
    "body": ["@Column(type: \"${1:varchar}\", length: ${2:255})"],
    "description": "Database column annotation"
  },
  "Id Annotation": {
    "prefix": "@Id",
    "body": ["@Id", "@Column(type: \"int\", autoIncrement: true)"],
    "description": "Database ID annotation"
  }
}
